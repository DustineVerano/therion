{
  "extends": ["react-native"],
  "settings": {
    "import/resolver": {
      "babel-module": {}
    }
  },
  "rules": {
    "semi": ["error", "always"],
    "quotes": ["error", "double"],
    "indent": ["error", "tab"],

    "react/prefer-stateless-function" : "off",
    "class-methods-use-this" : "off",
    "react/jsx-handler-names" : "off",
    "import/no-namespace" : "off",
    "import/prefer-default-export" : "off"
  }
}
